# functions
# colors (gnu)
eval $(dircolors $HOME/.dotfiles/dircolors/dircolors.ansi-dark)
zstyle ':completion:*' list-colors "${(s.:.)LS_COLORS}"

#keychain
if [ -e `which keychain` ]
then
	eval `keychain --eval --agents ssh id_rsa`
else
	echo -n "keychain is not installed, please install it first"
fi

#screenfetch
.dotfiles/bin/screenfetch-dev
